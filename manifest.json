{
  "manifest_version": 2,
  "name": "NoRefer",
  "description": "Set and exclude HTTP headers on all requests",
  "version": "1.3",
  "permissions": [
    "webRequest",
    "webRequestBlocking",
    "<all_urls>"
  ],
  "background": {
    "scripts": ["background.js"]
  },
  "browser_action": {
    "default_icon": "cat16.png",
    "default_title": "NoRefer",
    "default_popup": "panel.html"
  },
  "icons": {
    "16": "cat16.png",
    "48": "cat48.png",
    "128": "cat128.png"
  }
}
