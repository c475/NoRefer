{
  "manifest_version": 2,
  "name": "NoRefer",
  "description": "Set and exclude HTTP headers on all requests",
  "version": "1.1",
  "permissions": [
    "webRequest",
    "webRequestBlocking",
    "<all_urls>"
  ],
  "background": {
    "scripts": ["background.js"]
  },
  "browser_action": {
    "default_icon": "cat.png",
    "default_title": "NoRefer",
    "default_popup": "panel.html"
  }
}
